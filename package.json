{
  "name": "@ticklepoke/codemods",
  "version": "0.0.0-development",
  "description": "JavaScript codemods using Facebook's jscodeshift",
  "main": "index.js",
  "private": true,
  "repository": "https://github.com/ticklepoke/codemods.git",
  "author": "ticklepoke",
  "license": "MIT",
  "scripts": {
    "semantic-release": "semantic-release",
    "doc:gen": "node node_modules/markdown-include/bin/cli.js ./markdown.json",
    "transform:ls": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest",
    "plop": "plop",
    "build": "babel src --out-dir dist --extensions '.ts'",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/jscodeshift": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "markdown-include": "^0.4.3",
    "pinst": "^2.1.6",
    "plop": "^2.7.4",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.3",
    "semantic-release-cli": "^5.4.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "jscodeshift": "^0.12.0"
  }
}
